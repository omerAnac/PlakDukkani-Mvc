@model List<AdminAlbumVM>

@{
    ViewData["Title"] = "AdminIndex";
}




<section class="featured-artist-area section-padding-100 bg-img bg-overlay bg-fixed" style="background-image: url(img/bg-img/bg-4.jpg); height:100vh;">
    <div class="container">
        <div class="row align-items-center justify-content-center">
            <div class="col-12 m-5" style="height:600px; overflow: auto">
                <table class="table table-dark">
                    <thead>
                        <tr>
                            <th>ID</th>
                            <th>Title</th>
                            <th>Price</th>
                            <th>Stock</th>
                            <th>AlbumArtUrl</th>
                            <th>Continued</th>
                            <th>Discount</th>
                            <th>ArtistID</th>
                            <th>GenreID</th>
                            <th>Choise</th>
                        </tr>
                    </thead>
                    <tbody id="tableData">
                        <partial name="_adminAlbumTable" model="@Model" />
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</section>




