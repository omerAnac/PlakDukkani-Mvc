@model List<AdminAlbumVM>


@foreach (var item in Model)
{
    <tr>
        <td>@item.ID</td>
        <td>@item.Title</td>
        <td>@item.Price</td>
        <td>@item.Stock</td>
        <td>@item.AlbumArtUrl</td>
        <td>@item.Continued</td>
        <td>@item.Discount</td>
        <td>@item.ArtistID</td>
        <td>@item.GenreID</td>
        <td>
            <div class="btn-group" role="group" aria-label="Basic example" >
                <button type="button" class="btn btn-secondary"> <a  asp-action="Delete" asp-controller="Admin" asp-route-id="@item.ID" >Delete</a></button>
                <button type="button" class="btn btn-secondary"> <a asp-action="Edit" asp-controller="Admin" asp-route-id="@item.ID">Edit</a></button>
            </div>
        </td>
    </tr>
}


